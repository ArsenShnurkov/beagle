#!/bin/sh

if [ -e ./@target@ ] && [ -e ./Makefile.am ] ; then
    echo "*** Running uninstalled @target@ ***"
    THIS_PATH="../Util:../images:../Lucene.Net:../Filters:../indexer:../Tiles"
    THIS_EXE="./@target@"
else
    THIS_PATH="@pkglibdir@"
    THIS_EXE="@pkglibdir@/@target@"
fi

MONO_PATH="$THIS_PATH:$MONO_PATH" mono --debug $THIS_EXE "$@"

